    "unit"   : "proyectos"
  },
  "filters" : [
    {"type" : "state", "field" : "ID_ENTIDAD_FEDERATIVA"},
    {"type" : "city", "field" : "ID_MUNICIPIO"},
    {"type" : "year", "field" : "CICLO"}
  ],
  "extraFilters" : [
    {"type" : "extra", "field" : "DESC_PP", "title" : "Fondo"}
  ],
   "defaultFilters" : [
    {"type" : "year", "value" : 2017, "parentFilter" : null}
  ],
  "graphs" : {
    "graph1" : {
      "title" : "Monto Aprobado",
      "xAxis" : {
        "field" : "CICLO",
