      },
      "yAxis" : {
        "field" : "MONTO_APROBADO",
        "title" : "Monto Aprobado",
        "type"  : null
      },
      "zAxis" : {
        "field" : "DESC_PP",
        "title" : "Fondo",
        "type"  : "branch"
      },
      "maxLocations" : 5,
      "colors" : ["red", "green", "blue", "yellow", "purple", "pink", "black", "orange", "gray", "maroon"]
    },
    "graph2" : {
      "title" : "Monto total",
      "value" : "MONTO_APROBADO",
      "area" : {
        "field" : "DEC_PP",
        "title" : "Fondo",
        "type"  : null
      },
      "maxLocations" : 5,
      "colors" : ["red", "green", "blue", "yellow", "purple", "pink", "black", "orange", "gray", "maroon", "aquamarine", "chartreuse", "cyan", "darkorange", "deeppink", "gold"]
    }
  },
   "graph3" : {
      "title" : "Monto Pagado por Fondo",
      "xAxis" : {
        "field" : "CICLO",
        "title" : "Ciclo",
        "type"  : null
      },
      "yAxis" : {
        "field" : "MONTO_PAGADO",
        "title" : "Monto Pagado"
      },
      "zAxis" : {
        "field" : "DESC_PP",
        "title" : "Fondo",
        "type"  : "branch"
      },
      "maxLocations" : 5,
      "colors" : ["#bcce00", "#ee7336", "#25ae85", "#5aa099", "#462b5f", "#f49d51", "#008789", "#b3a493", "#1c9059", "#dc1918", "#b8c9d2","#75bf8d"]
    },
 "graph4" : {
      "title" : "Monto Aprobado por AÃ±o",
      "value" : "MONTO_APROBADO",
      "area" : {
        "field" : "DESC_PP",
        "title" : "Fondo",
        "type"  : "branch"
      },
      "points" : {
        "field" : "CICLO",
        "title" : "Ciclo",
        "type"  : null
      },
      "maxLocations" : 5,
      "colors" : ["#bcce00", "#ee7336", "#25ae85", "#5aa099", "#462b5f", "#f49d51", "#008789", "#b3a493", "#1c9059", "#dc1918", "#b8c9d2","#75bf8d"]
    },
  "template" : "<%=name%> <div class='amount_label'><h5> Proyectos: <strong><%=value%></strong></h5></div><div class='amount_label'><h5> Pagado: <strong>$<%=MONTO_PAGADO%></strong></h5></div><div class='amount_label'><h5> Aprobado: <strong>$<%=MONTO_APROBADO%></strong></h5></div>"
}
