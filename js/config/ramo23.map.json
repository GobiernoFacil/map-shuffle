{
  "name"  : "Ramo 23",
  "type"  : "area",
  "level" : ["state", "city"],
  "src"   : "/js/maps/ramo23-2017.csv",
  "file"  : "csv",
  "color" : 12,
  "location" : {
    "state" : "id_entidad",
    "city"  : "id_municipio"
  },
  "style" : {
    "fillOpacity":0.8
  },
  "values"  : ["monto", "Pagado"],
  "data"    : ["clave_de_programa", "Fondo", "entidad", "municipio", "proyecto"],
  "current" : {
    "level"  : "city",
    "value"  : "monto",
    "method" : "length",
    "unit"   : "proyectos"
  },
  "filters" : [
    {"type" : "state", "field" : "id_entidad"},
    {"type" : "city", "field" : "id_municipio"}
  ],
  "extraFilters" : [
    {"type" : "extra", "field" : "Fondo", "title" : "Clave de programas"}
  ],
  "graphs" : {
    "graph1" : {
      "title" : "Monto aprobado en 2017",
      "xAxis" : {
        "field" : "monto",
        "title" : "monto total invertido"
      },
      "yAxis" : {
        "field" : "Fondo",
        "title" : "Fondo",
        "type"  : null
      },
      "zAxis" : null,
      "maxLocations" : 5,
      "colors" : ["red", "green", "blue", "yellow", "purple", "pink", "black", "orange", "gray", "maroon"]
    },
    "graph2" : {
      "title" : "Monto total",
      "value" : "monto",
      "area" : {
        "field" : "Fondo",
        "title" : "Fondo",
        "type"  : null
      },
      "maxLocations" : 5,
      "colors" : ["red", "green", "blue", "yellow", "purple", "pink", "black", "orange", "gray", "maroon", "aquamarine", "chartreuse", "cyan", "darkorange", "deeppink", "gold"]
    }
  },
  "template" : "<%=name%> <div class='amount_label'><h5> Registros: <strong><%=value%></strong></h5></div"
}