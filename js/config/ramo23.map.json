{
  "name"  : "Ramo 23",
  "type"  : "area",
  "level" : ["state", "city"],
  "src"   : "/js/maps/ramo23_comp.csv",
  "file"  : "csv",
  "color" : 12,
  "colorLevels" : 5,
  "location" : {
    "state" : "ID_ENTIDAD_FEDERATIVA",
    "city"  : "ID_MUNICIPIO"
  },
  "style" : {
    "fillOpacity":0.8
  },
  "values"  : ["MONTO_APROBADO", "MONTO_PAGADO"],
